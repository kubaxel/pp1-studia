class Music():
    def __init__(self,artist,track_title,album,year):
        self.artist=artist
        self.track_title=track_title
        self.album=album
        self.year=year
    
    def __str__(self):
        a="Performer: " + self.artist 
        b="Song: "+ self.track_title
        c="Album: "+ self.album
        d="Year: "+self.year

        return (f'{a}\n{b}\n{c}\n{d}')


a=Music("Eoeoe","asdasd","ddsssd","sdawed")

print(a)