a=[
    [3,9,2],
    [2,4,5],
    [7,1,6],
    [0,4,8]
    ]
b=0
for i in a[0]:
    c=int(i)
    if c%2==0:
        b=b+c
for i in a[1]:
    c=int(i)
    if c%2==0:
        b=b+c
for i in a[2]:
    c=int(i)
    if c%2==0:
        b=b+c
for i in a[3]:
    c=int(i)
    if c%2==0:
        b=b+c
print(b)
